{
  "name": "file_analyzer",
  "version": "1.0.0",
  "description": "A server that returns a File's metadata",
  "main": "index.js",
  "scripts": {
    "start": "npm run build && npm run start-server",
    "build": "npx tsc",
    "start-server": "node dist/src/server.js",
    "dev": "concurrently npm:watch*",
    "watch": "nodemon -q dist/src/server.js",
    "watch-ts": "npx tsc --watch",
    "test": "npx jest --randomize"
  },
  "keywords": [
    "freecodecamp",
    "file_analyzer"
  ],
  "author": "Ruben S. Garcia",
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.1",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/pug": "^2.0.6",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "concurrently": "^8.0.1",
    "eslint": "^8.39.0",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "pug": "^3.0.2",
    "ts-node": "^10.9.1",
    "winston": "^3.8.2"
  }
}
